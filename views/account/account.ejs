<main>
  <% if (messages) { %>
    <% Object.keys(messages).forEach(type => { %>
      <div class="<%= type %>">
        <% messages[type].forEach(msg => { %>
          <p><%= msg %></p>
        <% }) %>
      </div>
    <% }) %>
  <% } %>

  <% if (errors && !errors.isEmpty()) { %>
    <ul>
      <% errors.array().forEach(error => { %>
        <li><%= error.msg %></li>
      <% }) %>
    </ul>
  <% } %>

  <h1><%= title %></h1>

  <p>You're logged in.</p>
</main>